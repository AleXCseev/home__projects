{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport styles from \"./Up.module.css\";\nimport { useScrollY } from \"../hooks/useScrollY\";\nimport { motion, useAnimation } from \"framer-motion\";\nimport React, { useEffect } from \"react\";\nimport { ButtonIcon } from \"..\";\nexport var Up = function Up() {\n  var controls = useAnimation();\n  var y = useScrollY();\n  useEffect(function () {\n    controls.start({\n      opacity: y / document.body.scrollHeight\n    });\n  }, [y, controls]);\n\n  var scrollToTop = function scrollToTop() {\n    window.scrollTo({\n      top: 0,\n      behavior: \"smooth\"\n    });\n  };\n\n  return /*#__PURE__*/_jsx(motion.div, {\n    className: styles.up,\n    animate: controls,\n    initial: {\n      opacity: 0\n    },\n    children: /*#__PURE__*/_jsx(ButtonIcon, {\n      appearance: \"primary\",\n      icon: \"up\",\n      onClick: scrollToTop,\n      \"aria-label\": \"\\u041D\\u0430\\u0432\\u0435\\u0440\\u0445\"\n    })\n  });\n};","map":null,"metadata":{},"sourceType":"module"}